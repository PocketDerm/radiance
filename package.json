{
  "name": "radiance-ui",
  "version": "1.0.1",
  "description": "Curology's React based component library",
  "main": "dist/bundle.js",
  "module": "dist/bundle.es.js",
  "files": [
    "dist",
    "lib",
    "lib/constants",
    "lib/utils"
  ],
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "clean": "rimraf dist lib",
    "build": "run-s clean && run-p build:rollup build:babel",
    "build:rollup": "rollup -c",
    "build:babel": "babel src -d lib",
    "publish-package": "run-s build && yarn version && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PocketDerm/radiance-ui.git"
  },
  "keywords": [
    "curology",
    "component-library",
    "react"
  ],
  "author": "Curology <engineering@curology.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PocketDerm/radiance-ui/issues"
  },
  "homepage": "https://github.com/PocketDerm/radiance-ui#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "4.0.0-alpha.25",
    "@storybook/addon-info": "4.0.0-alpha.25",
    "@storybook/addon-knobs": "4.0.0-alpha.25",
    "@storybook/addon-links": "4.0.0-alpha.25",
    "@storybook/react": "4.0.0-alpha.25",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "circular-dependency-plugin": "^5.0.2",
    "emotion": "^9.2.12",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-curology": "1.x.x",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-curology": "^0.0.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "lodash.round": "^4.0.4",
    "prettier": "1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-emotion": "^9.2.12",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "tinycolor2": "^1.4.1",
    "webpack": "^4.23.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "subTaskConcurrency": 1,
    "linters": {
      "resources/**/!(style|styles|test).js": [
        "prettier --write --single-quote",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
